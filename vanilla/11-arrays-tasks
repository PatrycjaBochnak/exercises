const cars = "Audi, Mercedes, BMW, Nissan, Dodge";

const newCars = cars.split(", ");

newCars.length > 3 ? console.log('jest OK') : console.log('Nie jest OK')

if (newCars.includes('Audi')) {
    newCars.push("Lambo")
} else {
    newCars.pop()
}

